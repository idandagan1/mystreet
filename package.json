{
  "name": "mystreet",
  "version": "1.3.4",
  "scripts": {
    "build": "better-npm-run build",
    "build:es6": "babel ./server/server-es6.js -o ./server/server.js",
    "build:server": "NODE_ENV=production webpack -p --config webpack.production.server.js --progress",
    "build:client": "NODE_ENV=production webpack -p --config webpack.production.config.js --progress",
    "postinstall": "npm run build",
    "start": "BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js --exec babel-node --presets es2015,stage-2",
    "start:prod": "NODE_ENV=production babel-node index.js",
    "clean": "rimraf dist/* && rimraf ./server/server.js"
  },
  "betterScripts": {
    "build": {
      "command": "npm run build:es6 && npm run build:client",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idandagan1/MyStreet"
  },
  "author": "Idan Dagan and Stephen Lenrow",
  "license": "ISC",
  "engines": {
    "node": "8.9.4"
  },
  "dependencies": {
    "better-npm-run": "^0.1.0",
    "body-parser": "^1.15.2",
    "clean-webpack-plugin": "^0.1.16",
    "connect-mongo": "^1.3.2",
    "css-loader": "^0.23.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-session": "^1.14.2",
    "express-validator": "^5.0.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.22.0",
    "jquery": "^3.1.1",
    "jquery-ui": "^1.12.1",
    "moment": "^2.17.1",
    "mongoose": "^4.6.5",
    "morgan": "^1.7.0",
    "node-sass": "^3.8.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "path": "^0.12.7",
    "react": "^15.3.0",
    "react-datepicker": "^0.53.0",
    "react-dom": "^15.3.0",
    "react-facebook-login": "^3.6.2",
    "react-facebook-login-component": "^0.9.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "sha1": "^1.1.1",
    "style-loader": "^0.13.1",
    "superagent": "^3.6.3",
    "underscore": "^1.8.3",
    "universal-cookie": "^2.1.0",
    "url-loader": "^0.6.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^3.1.6"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.11.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "@decorators/express": "^1.3.1",
    "nodemon": "^1.11.0",
    "css-loader": "^0.23.1",
    "react-input-range": "^1.2.1"
  }
}
